<h1>Option Analysis</h1>
<p>All prices are in USD</p>
<hr />
<div>
    <form>
        <span>Search:</span>
        <input class="form-control-autocomplete" type="text" placeholder="Press Enter to Add"
               ng-change="OnInputChange()" ng-enter="SelectSymbol()" ng-model="selected"
               typeahead="symbol as symbol.Name + ' - ' + symbol.Symbol for symbol in symbols | limitTo: 5" autofocus />
    </form>
</div>
<circle></circle>
<div ng-hide="hasOptionData()">
    <hr />
    <p>Please search for a company to view its option table.</p>
</div>
<div>
    <hr />
    @if (Model != null)
    {
        Html.RenderAction("Grid", "Grid", new { data = Model });
    }
</div>